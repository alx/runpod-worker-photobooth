{
  "input": {
    "image": "",
    "prompt": "professional headshot photo of a person in business attire, studio lighting, clean background, high quality portrait",
    "num_inference_steps": 4,
    "guidance_scale": 2.0,
    "width": 1024,
    "height": 1024,
    "use_controlnet": true,
    "controlnet_types": ["depth", "openpose"],
    "controlnet_conditioning_scale": 0.5,
    "swap_faces": true,
    "source_indexes": "-1",
    "target_indexes": "-1",
    "face_restore": true,
    "face_upsample": true,
    "background_enhance": true,
    "output_format": "JPEG"
  }
}